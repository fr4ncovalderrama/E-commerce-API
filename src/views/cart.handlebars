<div class="container py-4">
    <h1>Esto es lo que llevas</h1>
      <div class="products row">
       <table class="table">
          <thead>
            <tr>
              <th>Title</th>
              <th>Price</th>
              <th>Cantidad</th>
            </tr>
          </thead>
          <tbody>
            {{#each cartProducts}}
            <tr>
                <td>{{this.product.title}}</td>
                <td>${{this.product.price}}</td>
                <td>{{this.quantity}}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>
    </div>
    </div>